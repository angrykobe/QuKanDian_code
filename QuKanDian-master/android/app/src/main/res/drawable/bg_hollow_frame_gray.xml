<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 圆角 -->
    <corners
        android:radius="4dp"
        /><!-- 设置圆角半径 -->
    <!-- 填充 -->
    <solid
        android:color="#00000000"/>
    <!-- 填充的颜色 -->
    <!-- 描边 dashWidth：实线宽度   dashGap,虚线宽度-->
    <stroke
        android:width="1px"
        android:color="#bfbfbf"
        android:dashWidth="0px"
        android:dashGap="0px"/>

</shape>